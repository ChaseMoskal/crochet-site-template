{
	"version": "0.1.0",
	"command": "npm",
	"isShellCommand": true,
	"showOutput": "always",
	"suppressTaskName": true,
	"tasks": [
		{
			"taskName": "build (production)",
			"args": ["install"]
		},
		{
			"taskName": "build (debug)",
			"args": ["run", "build-debug", "-s"],
			"isBuildCommand": true
		},
		{
			"taskName": "test",
			"args": ["test"]
		}
	]
}
